set showcmd
set showmatch
set incsearch
set hlsearch
set infercase
set nojoinspaces
set mouse=a
set autoindent
set copyindent
set smartindent
set number
set shiftwidth=2
set tabstop=2
set splitright
set softtabstop=2
set shiftround
set smarttab
set expandtab
set ruler
set title
set icon
set scrolloff=5
set showmode
set sidescroll=5
set nowrap
syntax on

set background=dark
colorscheme solarized


set nocompatible
filetype plugin indent on
syntax on
let clj_highlight_builtins = 1

